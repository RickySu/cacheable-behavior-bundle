<?xml version="1.0" encoding="UTF-8"?>
<database name="cacheable_behavior_database" defaultIdMethod="native">
    <table name="singleuniquekey">
        <column name="id" required="true" primaryKey="true" type="INTEGER" />
        <column name="key1" required="true" type="INTEGER" /> 
        <unique>
            <unique-column name="key1" />
        </unique>                
        <behavior name="cacheable">
            <parameter name="primarykey_cache" value="true"/>
            <parameter name="uniqueindex_cache" value="true"/>
        </behavior>
    </table>
</database>
