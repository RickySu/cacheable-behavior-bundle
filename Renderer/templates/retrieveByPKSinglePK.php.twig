

    /**
     * Retrieve a single object by pkey.
     *
     * @param      {{PK.getPhpType()}} $pk the primary key.
     * @param      PropelPDO $con the connection to use
     * @return     {{peerBuilder.getObjectClassname()}}
     */
    public static function {{peerBuilder.getRetrieveMethodName()}}($pk, PropelPDO $con = null)
    {
        $CacheKey="Model:{{peerBuilder.getObjectClassname()}}:$pk";
        $Cache=self::getTagcache();
        if($Obj=$Cache->get($CacheKey)){
            return $Obj;
        }
        if($Obj=self::rebuild_{{peerBuilder.getRetrieveMethodName()}}($pk,$con)){
            $Cache->set($CacheKey,$Obj);
        }
        return $Obj;
    }
